<%- include("header") -%>

    <div class="class" id="heading">
        <h1>
            <%= listTitle %>
        </h1>
    </div>

    <div class="box">
        <% for(let i=0; i <newItemLists.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newItemLists[i] %>
                        <p>
            </div>
            <% } %>

                <form class="" action="/" method="post">
                    <input type="text" name="newItem" placeholder="Enter New Item" autocomplete="off">
                    <button type="submit" name="list" value=<%=listTitle %>>+</button>
                </form>
    </div>

    <%- include("footer") -%>